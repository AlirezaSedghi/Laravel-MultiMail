{
    "name": "iwasherefirst2/multimail",
    "description": "A package to send mails easily from multiple mail accounts with Laravel",
    "keywords": ["multiple providers", "laravel", "emails"],
    "license": "MIT",
    "homepage": "",
    "support": {
        "issues": "",
        "source": ""
    },
    "authors": [
        {
            "name": "Dr. Adam Nielsen",
            "email": "info@drnielsen.de"
        }
    ],
    "require": {
        "illuminate/support": "~5.6.0",
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7.14|~6.1|~7.0|~8.0",
        "laravel/framework": "~5.6.0|~5.7.0|~5.8.0",
    },
    "autoload": {
        "psr-4": {
            "IWasHereFirst2\\MultiMail\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IWasHereFirst2\\MultiMail\\Tests\\": "tests/"
      }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "IWasHereFirst2\\MultiMail\\MultiMailServiceProvider"
            ],
            "aliases":{
              "MultiMail": "IWasHereFirst2\\MultiMail\\MultiMailer"
            }
        }
    }
}
